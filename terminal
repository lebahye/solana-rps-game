# Make sure you're in the project root directory
cd solana-rps-game

# Initialize git if not already done
git init

# Add all files
git add .

# Initial commit
git commit -m "Initial project commit"

# Set the main branch
git branch -M main

# Add the remote (your repository)
git remote add origin https://github.com/lebahye/solana-rps-game.git

# Push to GitHub
git push -u origin main
